{
  "expo": {
    "name": "Om-De-Hoek",
    "slug": "Om-De-Hoek",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "omdehoek",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-localization"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "be.omdehoek.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Deze app heeft de camera nodig om foto's te kunnen maken.",
        "NSUserNotificationsUsageDescription": "Deze app gebruikt notificaties om je op de hoogte te houden van updates.",
        "NSLocationWhenInUseUsageDescription": "Deze app heeft je locatie nodig om je relevante info in de buurt te tonen."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "be.omdehoek.app",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c42275a0-d389-4d08-90c1-69b1548af0e7"
      }
    },
    "owner": "om-de-hoek",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c42275a0-d389-4d08-90c1-69b1548af0e7",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
